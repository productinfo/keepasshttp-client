{
  "name": "keepasshttp-client",
  "version": "2.1.0",
  "description": "Node.js module for interaction with KeePassHTTP",
  "author": {
    "name": "Vladimir Yakovlev",
    "email": "dev.vladimir.y@gmail.com"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/vladimiry/keepasshttp-client/blob/master/LICENSE-MIT"
    }
  ],
  "main": "./client.js",
  "types": "./client.d.ts",
  "engines": {
    "node": ">=6.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:vladimiry/keepasshttp-client.git"
  },
  "keywords": [
    "keepass",
    "password",
    "security"
  ],
  "scripts": {
    "compile": "tsc --listEmittedFiles --sourceMap --outDir ./compile",
    "compile:watch": "tsc --listEmittedFiles --sourceMap --outDir ./compile -w",
    "compile:test": "tsc --listEmittedFiles --outDir ./compile/test -p ./tsconfig-test.json",
    "dist:clean": "rimraf ./dist",
    "dist:copy": "cpx \"{package.json,README.md,LICENSE-MIT}\" ./dist",
    "dist:build": "tsc --listEmittedFiles --noEmitOnError --declaration --outDir ./dist",
    "dist": "npm-run-all lint test dist:clean dist:copy dist:build",
    "lint": "tslint \"./src/**/*.ts\"",
    "test:tape": "tape \"./compile/test/**/*.{spec,test}.js\"",
    "test": "npm-run-all compile:test test:tape"
  },
  "devDependencies": {
    "@types/node-fetch": "1.6.7",
    "@types/tape": "4.2.31",
    "cpx": "1.5.0",
    "npm-run-all": "4.1.2",
    "rimraf": "2.6.2",
    "tape": "4.8.0",
    "tslint": "5.8.0",
    "tslint-eslint-rules": "4.1.1",
    "typescript": "2.6.1"
  },
  "dependencies": {
    "node-fetch": "1.7.3"
  }
}
