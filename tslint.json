{
    "extends": [
        "tslint-eslint-rules",
        "tslint:recommended"
    ],
    "rules": {
        "variable-name": [
            true,
            "allow-leading-underscore"
        ],
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "member-access": false,
        "max-classes-per-file": [
            false
        ],
        "interface-name": false,
        "arrow-parens": [
            true
        ]
    }
}
